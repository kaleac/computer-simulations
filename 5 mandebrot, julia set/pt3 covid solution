    "nMax = 75\n",
    "N = 201\n",
    "C = complex(-0.25, 0.75)\n",
    "data = np.zeros((N,N))\n",
    "x = np.linspace(0.05, 0.0509, N)\n",
    "y = np.linspace(0.15, 0.1509, N)\n",
    "\n",
    "def f4(z, C):\n",
    "    div = []\n",
    "    for i in range(nMax):\n",
    "        z = Mandelbrot(z, C)\n",
    "        z1 = abs(z)\n",
    "        if (z1 > 2):\n",
    "            return i\n",
    "        else:\n",
    "            div.append(z)\n",
    "    return nMax\n",
    "#print(C)\n",
    "for i in range(N):\n",
    "    for j in range(N):\n",
    "        z_val = complex(x[i], y[j])\n",
    "        vis = f4(z_val, C)\n",
    "        data[i,j] = vis\n",
    "        \n",
    "print(\"z0 interval:\", z)\n",
    "print(\"C parameter:\", C)     \n",
    "\n",
    "plt.imshow(data, cmap = 'inferno')"
   ]
