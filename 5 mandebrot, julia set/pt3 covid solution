    "nMax = 100\n",
    "N = 201\n",
    "C = complex(-0.85, 0.22)\n",
    "data = np.zeros((N,N))\n",
    "x = np.linspace(0.1789, 0.1798, N)\n",
    "y = np.linspace(-0.0311, -0.032, N)\n",
    "\n",
    "def f4(z, C):\n",
    "    div = []\n",
    "    for i in range(nMax):\n",
    "        z = Mandelbrot(z, C)\n",
    "        z1 = abs(z)\n",
    "        if (z1 > 2):\n",
    "            return i\n",
    "        else:\n",
    "            div.append(z)\n",
    "    return nMax\n",
    "#print(C)\n",
    "for i in range(N):\n",
