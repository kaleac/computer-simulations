   "source": [
    "from matplotlib import cm\n",
    "N = 201\n",
    "nMax = 100\n",
    "C = complex(-0.4, 0.6)\n",
    "data = np.zeros((N,N))\n",
    "x = np.linspace(-1.5, 1.5, N)\n",
    "y = np.linspace(-1.5, 1.5, N)\n",
    "\n",
    "def f4(z, C):\n",
    "    div = []\n",
    "    for i in range(nMax):\n",
    "        z = Mandelbrot(z, C)\n",
    "        z1 = abs(z)\n",
    "        if (z1 > 2):\n",
    "            return i\n",
    "        else:\n",
    "            div.append(z)\n",
    "    return nMax\n",
    "#print(C)\n",
    "for i in range(N):\n",
    "    for j in range(N):\n",
    "        z_val = complex(x[i], y[j])\n",
    "        vis = f4(z_val, C)\n",
    "        data[i,j] = vis\n",
    "\n",
    "#fig, ax = plt.subplots()\n",
    "\n",
    "#im = ax.imshow(data, interpolation='bilinear', cmap=cm.RdYlGn, origin='lower', extent=[-3, 3, -3, 3], vmax=abs(data).max(), vmin=-abs(data).max())\n",
    "\n",
    "plt.imshow(data)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "\n",
      "text/plain": [
       "<Figure size 432x288 with 1 Axes>"
      ]
     },
     "metadata": {
      "needs_background": "light"
     },
     "output_type": "display_data"
    }
   ],
   "source": [
    "# Image width and height; parameters for the plot\n",
    "N = 201\n",
    "C = complex(-0.4, 0.6)\n",
    "zMax = 2\n",
    "nMax = 150\n",
    "\n",
    "x = np.linspace(-3, 3, N)\n",
    "y = np.linspace(-3, 3, N)\n",
    "\n",
    "\n",
    "julia = np.zeros((N, N))\n",
    "for ix in range(N):\n",
    "    for iy in range(N):\n",
    "        niter = 0\n",
    "        # Map pixel position to a point in the complex plane\n",
    "        z = complex(x[ix], y[iy])\n",
    "        # Do the iterations\n",
    "        while abs(z) <= zMax and niter < nMax:\n",
    "            z = z**2 + C\n",
    "            niter += 1\n",
    "        #shade = 1-np.sqrt(niter / nMax)\n",
    "        ratio = niter / nMax\n",
    "        julia[ix,iy] = ratio\n",
    "\n",
    "plt.imshow(julia, interpolation='nearest')\n",
    "plt.show()"
   ]
  },
