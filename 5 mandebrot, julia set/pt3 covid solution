   "source": [
    "N = 201\n",
    "C = complex(-0.25, 0.75)\n",
    "zMax = 2\n",
    "nMax = 35\n",
    "\n",
    "x = np.linspace(-1.7, 1.7, N)\n",
    "y = np.linspace(-1.7, 1.7, N)\n",
    "\n",
    "\n",
    "julia = np.zeros((N, N))\n",
    "for ix in range(N):\n",
    "    for iy in range(N):\n",
    "        niter = 0\n",
    "        # Map pixel position to a point in the complex plane\n",
    "        z = complex(x[ix], y[iy])\n",
    "        # Do the iterations\n",
    "        while abs(z) <= zMax and niter < nMax:\n",
    "            z = z**2 + C\n",
    "            niter += 1\n",
    "        #shade = 1-np.sqrt(niter / nMax)\n",
    "        ratio = niter / nMax\n",
    "        julia[ix,iy] = ratio\n",
    "\n",
    "print(\"z0 interval:\", z)\n",
    "print(\"C parameter:\", C)\n",
    "\n",
    "plt.imshow(julia, interpolation='nearest', cmap = 'inferno')\n",
    "plt.show()"
   ]
