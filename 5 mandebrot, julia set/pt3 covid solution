    "nMax = 150\n",
    "C = complex(-0.4, 0.6)\n",
    "data = np.zeros((N,N))\n",
    "x = np.linspace(-0.4301, -0.43001, N)\n",
    "y = np.linspace(-0.2001, -0.20001, N)\n",
    "\n",
    "def f4(z, C):\n",
    "    div = []\n",
    "    for i in range(nMax):\n",
    "        z = Mandelbrot(z, C)\n",
    "        z1 = abs(z)\n",
    "        if (z1 > 2):\n",
    "            return i\n",
    "        else:\n",
